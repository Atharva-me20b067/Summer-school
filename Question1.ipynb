    "from numpy import random\n",
    "\n",
    "import numpy as np\n",
    "y_cap = np.random.rand(100)\n",
    "y = np.random.randint(2,size = 100)\n",
    "y1 = np.log2(y_cap)\n",
    "y2 = np.log2(1-y_cap)\n",
    "O = (-1/100)*((y*y1) + (1-y)*y2)\n",
    "print(O.sum())"
   ]
